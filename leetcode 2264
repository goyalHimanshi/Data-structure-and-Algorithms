class Solution {
public:
    string largestGoodInteger(string num) {
        int n=num.length();
        char maxval=' ';
        for(int i=2;i<n;i++){
            if(num[i]==num[i-1] &&num[i]==num[i-2]){
                maxval=max(maxval,num[i]);
            }
        }
        if(maxval==' '){
            return "";
        }
        return string(3,maxval);
        
    }
};
